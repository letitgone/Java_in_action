<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="aop.mapper.ExceptionLogsMapper">    <resultMap id="exceptionLogsResultMap" type="aop.domain.ExceptionLogs">        <id     column="exception_log_id"     property="exceptionLogId"/>        <result column="log_token"          property="logToken"/>        <result column="exception_time"       property="exceptionTime"/>        <result column="exception_info"       property="exceptionInfo"/>        <result column="stack_track"        property="stackTrack"/>    </resultMap>    <insert id="insertExceptionLogs" parameterType="aop.domain.ExceptionLogs">        insert into exception_log (            <if test="exceptionLogId != null and exceptionLogId != 0">exception_log_id,</if>            <if test="logToken != null and logToken != ''">log_token,</if>            <if test="exceptionTime != null and exceptionTime != ''">exception_time,</if>            <if test="exceptionInfo != null and exceptionInfo != ''">exception_info,</if>            <if test="stackTrack != null and stackTrack != ''">stack_track</if>        ) values (            <if test="exceptionLogId != null and exceptionLogId != ''">#{exceptionLogId},</if>            <if test="logToken != null and logToken != ''">#{logToken},</if>            <if test="exceptionTime != null and exceptionTime != ''">#{exceptionTime},</if>            <if test="exceptionInfo != null and exceptionInfo != ''">#{exceptionInfo},</if>            <if test="stackTrack != null and stackTrack != ''">#{stackTrack}</if>        )    </insert></mapper>