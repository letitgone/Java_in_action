<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="aop.mapper.ResponseLogsMapper">    <resultMap id="responseLogsResultMap" type="aop.domain.ResponseLogs">        <id     column="response_log_id"     property="responseLogId"/>        <result column="log_token"          property="logToken"/>        <result column="response_time"       property="responseTime"/>        <result column="response_status"       property="responseStatus"/>        <result column="response_info"        property="responseInfo"/>        <result column="response_json"   property="responseJson"/>        <result column="response_json_byte"   property="responseJsonByte"/>        <result column="time_different"        property="timeDifferent"/>    </resultMap>    <insert id="insertResponseLogs" parameterType="aop.domain.ResponseLogs">        insert into response_log (            <if test="responseLogId != null and responseLogId != 0">response_log_id,</if>            <if test="logToken != null and logToken != ''">log_token,</if>            <if test="responseTime != null and responseTime != ''">response_time,</if>            <if test="responseStatus != null and responseStatus != ''">response_status,</if>            <if test="responseInfo != null and responseInfo != ''">response_info,</if>            <if test="responseJson != null and responseJson != ''">response_json,</if>            <if test="responseJsonByte != null and responseJsonByte != ''">response_json_byte,</if>            <if test="timeDifferent != null and timeDifferent != ''">time_different</if>        ) values (            <if test="responseLogId != null and responseLogId != 0">#{responseLogId},</if>            <if test="logToken != null and logToken != ''">#{logToken},</if>            <if test="responseTime != null and responseTime != ''">#{responseTime},</if>            <if test="responseStatus != null and responseStatus != ''">#{responseStatus},</if>            <if test="responseInfo != null and responseInfo != ''">#{responseInfo},</if>            <if test="responseJson != null and responseJson != ''">#{responseJson},</if>            <if test="responseJsonByte != null and responseJsonByte != ''">#{responseJsonByte},</if>            <if test="timeDifferent != null and timeDifferent != ''">#{timeDifferent}</if>        )    </insert></mapper>