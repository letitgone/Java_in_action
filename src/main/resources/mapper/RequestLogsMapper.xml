<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="aop.mapper.RequestLogsMapper">    <resultMap id="requestLogsResultMap" type="aop.domain.RequestLogs">        <id     column="request_log_id"     property="requestLogId"/>        <result column="log_token"          property="logToken"/>        <result column="request_time"       property="requestTime"/>        <result column="request_json"       property="requestJson"/>        <result column="request_json_byte"  property="requestJsonByte"/>        <result column="request_way"        property="requestWay"/>        <result column="request_platform"   property="requestPlatform"/>        <result column="request_url"        property="requestUrl"/>        <result column="request_operator"   property="requestOperator"/>    </resultMap>    <insert id="insertRequestLogs" parameterType="aop.domain.RequestLogs">        insert into request_log (            <if test="requestLogId != null and requestLogId != 0">request_log_id,</if>            <if test="logToken != null and logToken != ''">log_token,</if>            <if test="requestTime != null and requestTime != ''">request_time,</if>            <if test="requestJson != null and requestJson != ''">request_json,</if>            <if test="requestJsonByte != null and requestJsonByte != ''">request_json_byte,</if>            <if test="requestWay != null and requestWay != ''">request_way,</if>            <if test="requestPlatform != null and requestPlatform != ''">request_platform,</if>            <if test="requestUrl != null and requestUrl != ''">request_url,</if>            <if test="requestOperator != null and requestOperator != ''">request_operator</if>        ) values (            <if test="requestLogId != null and requestLogId != ''">#{requestLogId},</if>            <if test="logToken != null and logToken != ''">#{logToken},</if>            <if test="requestTime != null and requestTime != ''">#{requestTime},</if>            <if test="requestJson != null and requestJson != ''">#{requestJson},</if>            <if test="requestJsonByte != null and requestJsonByte != ''">#{requestJsonByte},</if>            <if test="requestWay != null and requestWay != ''">#{requestWay},</if>            <if test="requestPlatform != null and requestPlatform != ''">#{requestPlatform},</if>            <if test="requestUrl != null and requestUrl != ''">#{requestUrl},</if>            <if test="requestOperator != null and requestOperator != ''">#{requestOperator}</if>        )    </insert></mapper>